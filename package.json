{
  "name": "liman-monorepo",
  "version": "1.0.0",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.11.1",
  "type": "module",
  "description": "Liman Monorepo",
  "scripts": {
    "prepare": "husky",
    "python:lint": "cd python && uv run poe lint",
    "python:typing": "cd python && uv run poe typing",
