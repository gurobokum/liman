    "commitlint:scope": "commitlint --print-config json | jq '.rules.\"scope-enum\"[2]'"
  },
  "keywords": [],
  "author": "Guro Bokum <jiojiajiu@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "husky": "^9.1.7"
  }
}
